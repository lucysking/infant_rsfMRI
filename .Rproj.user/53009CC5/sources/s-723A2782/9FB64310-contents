  postnatal_exp(mom_age_birth, lscr_rape1, lscr_rape1_age_off, lscr_rape1_preg) %>% 
  postnatal_exp(mom_age_birth, lscr_rape2, lscr_rape2_age_off, lscr_rape2_preg) %>% 
