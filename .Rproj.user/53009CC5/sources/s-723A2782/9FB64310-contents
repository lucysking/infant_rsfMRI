    lscr_adopt_preg,
    lscr_pardivorce_preg,
    lscr_divorce_preg,
    lscr_money_preg,
    lscr_ill_preg,
    lscr_emoab_preg,
    lscr_physneg_preg,
    lscr_abort_preg,
    lscr_sepchild_preg,
    lscr_care_preg,
    lscr_death1_preg,
    lscr_death2_preg,
    lscr_famviol_preg,
